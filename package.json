{
  "name": "my-vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:prod": "cross-env NODE_ENV=production vite build --mode production",
    "build:staging": "cross-env NODE_ENV=production vite build --mode staging",
    "serve:dev": "npm run build:dev && cross-env NODE_ENV=development vite preview --mode development",
    "serve:prod": "npm run build:prod && cross-env NODE_ENV=production vite preview --mode production",
    "serve:staging": "npm run build:staging && cross-env NODE_ENV=production vite preview --mode staging"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.8",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "path-browserify": "^1.0.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "url": "^0.11.4",
    "vite": "^4.5.5",
    "vite-plugin-svgr": "^3.3.0"
  }
}
